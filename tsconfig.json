{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "@components/*": ["./components/*"],
      "@app/*": ["./app/*"],
      "@utils/*": ["./utils/*"],
      "@lib/*": ["./lib/*"],
      "@assets/*": ["./assets/*"],
      "@constants/*": ["./constants/*"],
      "@config/*": ["./config/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
